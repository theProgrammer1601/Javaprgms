import javax.swing.*;
import java.awt.*;
public class Image1 extends JFrame
{
 public static void main(String args[])
 {
  JFrame jf = new JFrame("Image Icon");
  Icon icon = new ImageIcon("A.JPG");
  JLabel label1 = new JLabel("MARVEL STUDIOS PRESENTS ",icon,JLabel.CENTER);
  label1.setBounds(200, 200, 600, 600);
 
  jf.add(label1);
  jf.setLayout(null);
  jf.setSize(800,800);
  jf.setVisible(true);
 }
}
